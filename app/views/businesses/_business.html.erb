<div id="<%= dom_id business %>">
  <p>
    <strong>Name:</strong>
    <%= business.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= business.email %>
  </p>

  <p>
    <strong>Is owner:</strong>
    <%= business.is_owner %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= business.location %>
  </p>

  <p>
    <strong>Telephone:</strong>
    <%= business.telephone %>
  </p>

  <p>
    <strong>Website:</strong>
    <%= business.website %>
  </p>

  <p>
    <strong>Instagram:</strong>
    <%= business.instagram %>
  </p>

  <p>
    <strong>Opens at:</strong>
    <%= business.opens_at %>
  </p>

  <p>
    <strong>Closes at:</strong>
    <%= business.closes_at %>
  </p>

  <p>
    <strong>Business type:</strong>
    <%= business.business_type_id %>
  </p>

</div>
